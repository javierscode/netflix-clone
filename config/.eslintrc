{
    "extends": ["eslint:recommended","plugin:react/recommended"],
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "parserOptions": {
      "ecmaVersion": 9,
      "ecmaFeatures": {
        "jsx": true
      },
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/react-in-jsx-scope": 0,
      "no-console": "off",
      "strict": ["error", "global"],
      "curly": "warn"
    }
  }